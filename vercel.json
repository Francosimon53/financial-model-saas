{
    "version": 2,
    "buildCommand": "pnpm run vercel-build",
    "functions": {
        "server/index.ts": {
            "runtime": "@vercel/node@3.0.0"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}