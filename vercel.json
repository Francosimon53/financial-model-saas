{
    "buildCommand": "pnpm run vercel-build",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "s-maxage=0"
                }
            ]
        }
    ]
}